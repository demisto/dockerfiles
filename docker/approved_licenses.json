{
    "description": "Approved licenses file",
    "$schema": "./approved_licenses.schema.json",
    "licenses": [
        {
            "name": "BSD 3-Clause License",
            "url": "https://opensource.org/licenses/BSD-3-Clause",
            "regex": "BSD"
        },
        {
            "name": "Apache Software License, Version 2.0",
            "url": "https://www.apache.org/licenses/LICENSE-2.0",
            "regex": "Apache",
            "additional_urls": [
                "https://aka.ms/azps-license",
                "https://raw.githubusercontent.com/Azure/azure-powershell/dev/LICENSE.txt"
            ]
        },
        {
            "name": "MIT License",
            "url": "https://opensource.org/licenses/MIT",
            "regex": "MIT"
        },
        {
            "name": "Python Software Foundation License",
            "url": "https://docs.python.org/3/license.html",
            "regex": "Python Software Foundation|PSF license"
        },
        {
            "name": "Lesser General Public License (LGPL)",
            "url": "https://docs.python.org/3/license.html",
            "regex": "Lesser General Public License|LGPL"
        },
        {
            "name": "Mozilla Public License 2.0 (MPL 2.0)",
            "url": "https://www.mozilla.org/en-US/MPL/",
            "regex": "Mozilla Public License 2.0"
        },
        {
            "name": "Public Domain",
            "url": "https://en.wikipedia.org/wiki/Public-domain_software",
            "regex": "Public Domain"
        },
        {
            "name": "Academic Free License (AFL)",
            "url": "https://opensource.org/licenses/AFL-3.0",
            "regex": "Academic Free License"
        },
        {
            "name": "ISC License (ISCL)",
            "url": "https://opensource.org/licenses/ISC",
            "regex": "ISC License"
        },
        {
            "name": "zlib/libpng License",
            "url": "https://opensource.org/licenses/Zlib",
            "regex": "zlib/libpng"
        },
        {
            "name": "Zope Public License",
            "url": "https://opensource.org/licenses/ZPL-2.0",
            "regex": "Zope Public License"
        },
        {
            "name": "Creative Common \"Attribution\" license (CC-BY)",
            "url": "https://creativecommons.org/licenses/by/4.0/",
            "regex": "CC-BY"
        },
        {
            "name": "Artistic License",
            "url": "https://dev.perl.org/licenses/artistic.html",
            "regex": "Artistic License"
        },
        {
            "name": "Unlicense",
            "url": "https://unlicense.org",
            "regex": "Unlicense"
        },
        {
            "name": "Eclipse Public License v1.0 ",
            "url": "https://www.eclipse.org/legal/epl-v10.html",
            "regex": "Eclipse"
        }
    ]
